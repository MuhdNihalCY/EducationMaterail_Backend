<section>
    <!-- login popup -->
    <div class="Fullpage " id="myForm">
        <div class="form-popup">
            <form action="" id="login-form" method="post" class="form-container">
                <div class="LoginHead">
                    <h2 class="loginHeadText">Login</h2>
                    <img onclick="closeForm()" src="images/close-button.png" alt="" class="CloseButtonLogin">
                </div>
                <label for="email"><b>Email</b></label>
                <input type="email" id="LoginEmail" placeholder="Enter Email" name="email" required>

                <label for="psw"><b>Password</b></label>
                <input type="password" id="LoginPaasword" placeholder="Enter Password" name="psw" required>

                <button type="button" onclick="DoLogin()" class="btn">Login</button>
                <a class="btn" onclick="OpenSignup()">Sign Up</a>
                <!-- <button type="button" class="btn cancel" onclick="closeForm()">Close</button> -->
            </form>
        </div>
    </div>
    <!-- signup POpup -->
    <div class="SignupFullPage" id="Signup">
        <div class="form-popup">
            <form action="" id="signup-form" method="POST" class="form-container">
                <div class="LoginHead">
                    <h2 class="loginHeadText">Sign Up</h2>
                    <img onclick="closeForm()" src="images/close-button.png" alt="" class="CloseButtonLogin">
                </div>
                <label for="name"><b>name</b></label>
                <input id="name" type="text" placeholder="Enter name" name="email" required>

                <label for="email"><b>Email</b></label>
                <input id="email" type="email" placeholder="Enter Email" name="email" required>

                <label for="number"><b>Mob Number</b></label>
                <input id="number" type="number" placeholder="Enter number" name="number" required>


                <label for="psw"><b>Password</b></label>
                <input id="password" type="password" placeholder="Enter Password" name="psw" required>

                <button type="button" onclick="DoSignup()" class="btn">Sign Up</button>
                <a class="btn" onclick="openForm()">Login</a>
                <!-- <button type="button" class="btn cancel" onclick="closeForm()">Close</button> -->
            </form>
        </div>
    </div>


</section>
<!-- popup ends here -->


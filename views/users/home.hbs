<section class="SearchSection">
    <div class="container">
        <form action="/getFiles" method="post" enctype="multipart/form-data">
            <h2 class="FindText">Find Materials</h2>
            <div class="SearchArea row">

                <div class="RowSearch">
                    <div class="BranchSearchDiv">
                        <label for="Branch" class="SearchLabel">Branch</label>
                        <select required name="Branch" onchange="FilterChange()" class="custom-select" name=""
                            id="Branch">
                            <option value="" selected disabled>Select Branch</option>
                            <option value="ct">Computer Engineering</option>
                            <option value="ece">Electronic</option>
                            <option value="td">Tool and Die</option>
                        </select>
                    </div>

                    <div class="BranchSearchDiv">
                        <label for="Branch" class="SearchLabel">Revision</label>
                        <select required name="Revision" onchange="FilterChange()" class="custom-select" name=""
                            id="Revision">
                            <option value="" selected disabled>Select Revision</option>
                            <option value="2015">2015</option>
                            <option value="2021">2021</option>
                        </select>
                    </div>
                </div>

                <div class="RowSearch">
                    <div class="BranchSearchDiv">
                        <label for="Branch" class="SearchLabel">Semester</label>
                        <select required name="Semester" onchange="FilterChange()" class="custom-select" name=""
                            id="Semester">
                            <option value="" selected disabled>Select Semester</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="6">6</option>
                        </select>
                    </div>

                    <div class="BranchSearchDiv">
                        <label for="Branch" class="SearchLabel">Branch</label>
                        <select required name="Subject" class="custom-select" name="" id="Subject">
                            <option value="" selected disabled>Select Subject</option>

                        </select>
                    </div>
                </div>
                <div class="ApplyFilter">
                    <input type="submit" class="btn btn-primary" value="Search">
                </div>

            </div>
        </form>
    </div>
</section>
<!-- Search Section Ends here -->

<!-- Add File Starts HEre -->
<section>
    <div class="container">
        <div id="AddFormDiv" class="AddFormDiv">
            <form id="AddForm" action="/addFile" enctype="multipart/form-data" method="post">
                <div class="row">
                    <div class="MaterialType col-12 col-md-6">
                        <label for="MaterialType">Material Type</label>
                        <br>
                        <select required onchange="TypeSelected()" name="MaterialType" class="col-10" id="MaterialType">
                            <option value="" selected disabled>Select Type</option>
                            <option value="Video">Video</option>
                            <option value="Notes">Notes</option>
                            <option value="Q_P">Q&P</option>
                        </select>
                    </div>

                    <div id="link" class="LINK col-12 col-md-6">
                        <label for="Link">Material youtube Link</label>
                        <br>
                        <input id="Link" type="text" class="col-10" name="link" placeholder="Past Link Here">
                    </div>

                    <div id="File" class="uploadfile col-12 col-md-6">
                        <label for="file">Material file Upload</label>
                        <br>
                        <input id="file" type="file" accept="application/pdf" class="col-10" name="file">
                    </div>

                    <div id="title" class="title col-12 col-md-6">
                        <label for="Title">Material Title</label>
                        <br>
                        <input required id="Title" type="text" placeholder="Title" class="col-10" name="Title">
                    </div>

                    <div id="stream" class="title col-12 col-md-6">
                        <label for="Stream">Material Stream</label>
                        <br>
                        {{!-- Stream Selecting to add --}}
                        <select required name="stream" onchange="ADDFilterChange()" class="custom-select col-10" name="stream"
                            id="AddBranch">
                            <option value="" selected disabled>Select Branch</option>
                            <option value="ct">Computer Engineering</option>
                            <option value="ece">Electronic</option>
                            <option value="td">Tool and Die</option>
                        </select>
                        {{!-- <input required id="Stream" type="text" placeholder="Stream" class="col-10" name="stream">
                        --}}
                    </div>

                    <div id="sem" class="title col-12 col-md-6">
                        <label for="Sem">Material Semester</label>
                        <br>
                        {{!-- Add Semester --}}
                        <select required name="Sem" onchange="ADDFilterChange()" class="custom-select col-10" name="Sem"
                            id="AddSemester">
                            <option value="" selected disabled>Select Semester</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="6">6</option>
                        </select>
                        {{!-- <input required id="Sem" type="number" placeholder="Semester in digits" class="col-10"
                            name="Sem"> --}}
                    </div>

                    <div id="rev" class="title col-12 col-md-6">
                        <label for="Rev">Material Revision</label>
                        <br>
                        {{!--Add select Rev --}}
                        <select required name="Rev" onchange="ADDFilterChange()" class="custom-select col-10" name="Rev"
                            id="AddRevision">
                            <option value="" selected disabled>Select Revision</option>
                            <option value="2015">2015</option>
                            <option value="2021">2021</option>
                        </select>
                        {{!-- <input required id="Rev" type="number" placeholder="Revision in digits" class="col-10"
                            name="Rev"> --}}
                    </div>

                    <div id="sub" class="title col-12 col-md-6">
                        <label for="Sub">Material Subject</label>
                        <br>
                        {{!-- add Subject --}}
                        <select required name="Sub" class="custom-select col-10"  id="AddSubject">
                            <option value="" selected disabled>Select Subject</option>

                        </select>
                        {{!-- <input required id="Sub" type="text" placeholder="Subject" class="col-10" name="Sub"> --}}
                    </div>
                    <div class="submit">
                        <input type="submit" value="Add FIle" class="btn btn-primary col-12">
                    </div>
                    {{!-- <button id="AddBTN" onclick="AddMaterial()">Add Material</button> --}}
                </div>
            </form>
        </div>
    </div>
</section>
<!-- Add files Ends Here -->




